{
  "name": "webpack-chrome-extension",
  "version": "0.9.0",
  "description": "Bootstrap playground for developing Chrome extensions with all new features like Webpack, Babel, React and almost everything you want.",
  "private": true,
  "bin": {
    "chrome-extension": "./bin/chrome-extension"
  },
  "scripts": {
    "test": "TODO",
    "start": "./node_modules/.bin/babel src --out-dir lib --source-maps inline --presets es2015 --plugins transform-object-rest-spread --watch",
    "build": "./node_modules/.bin/babel src --out-dir lib --source-maps inline --presets es2015 --plugins transform-object-rest-spread",
    "example": "npm run build && ./bin/chrome-extension run ./example/manifest.json -o ./build",
    "example:build": "npm run build && ./bin/chrome-extension build ./example/manifest.json -o ./release",
    "prepublish": "npm run build"
  },
  "keywords": [
    "chrome",
    "extension",
    "chrome-extension",
    "babel",
    "webpack",
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr"
  ],
  "dependencies": {
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "chokidar": "1.6.0",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "css-loader": "0.23.1",
    "file-loader": "0.9.0",
    "findup-sync": "0.4.2",
    "fs-extra": "0.30.0",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "lodash": "4.14.0",
    "node-sass": "3.8.0",
    "react-hot-loader": "1.3.0",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-combine-loaders": "2.0.0",
    "webpack-dev-server": "1.14.1"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.11.4"
  }
}
